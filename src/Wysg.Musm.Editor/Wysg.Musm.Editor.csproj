    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\icoCodeSnippetRed.png" />
    <None Remove="Resources\icoHotkey.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\icoCodeSnippetRed.png">
